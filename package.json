    "chrome": "npm run production -- --browser=chrome",
    "firefox": "npm run production -- --browser=firefox",
    "chrome-staging": "npm run production -- --browser=chrome --envfile=staging",
    "firefox-staging": "npm run production -- --browser=firefox --envfile=staging"
  },
